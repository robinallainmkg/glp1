{
  "name": "glp1-affiliate-site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev --port 4321 --open",
    "dev:tina": "tinacms dev -c \"npm run dev\"",
    "build": "tinacms build && astro build",
    "preview": "astro preview --port 4321 --open",
    "start": "astro preview --port 4321 --open",
    "generate-database": "node scripts/generate-database-v2.mjs",
    "clean": "rm -rf dist .astro",
    "type-check": "astro check",
    "deploy": "node deploy.js",
    "deploy:auto": "node deploy-auto.js",
    "deploy:complete": "node deploy-complete.js",
    "deploy:full": "powershell -ExecutionPolicy Bypass -File deploy-full.ps1",
    "deploy:windows": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "deploy:mac": "bash deploy.sh",
    "optimize-images": "node scripts/optimize-images.mjs",
    "db:migrate": "node scripts/migrate-users.js",
    "db:test": "node scripts/test-supabase-config.js",
    "db:stats": "node -e \"import('./lib/services/userService.js').then(s => s.getUserStats().then(r => console.log(r.data)))\"",
    "analyze-users": "node scripts/user-data-analyzer.mjs",
    "optimize-images:responsive": "node scripts/optimize-images.mjs --responsive"
  },
  "dependencies": {
    "@astrojs/node": "^8.3.4",
    "@astrojs/sitemap": "^3.4.2",
    "@astrojs/tailwind": "^6.0.2",
    "@supabase/supabase-js": "^2.55.0",
    "@tinacms/auth": "^1.1.0",
    "@tinacms/cli": "^1.10.2",
    "astro": "^4.10.0",
    "dotenv": "^17.2.1",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "node-fetch": "^3.3.2",
    "openai": "^5.13.1",
    "papaparse": "^5.4.1",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.17",
    "tinacms": "^2.8.2"
  }
}
