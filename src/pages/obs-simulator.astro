                                <td><span class="priority-high">High</span></td>
                            </tr>
                        </tbody>
                    </table>
                </div>

                <!-- Urgent Symptoms Category -->
                <div class="category-section">
                    <div class="category-header">Urgent Symptoms</div>
                    <table class="keyword-table">
                        <thead>
                            <tr>
                                <th>Keyword</th>
                                <th>Volume/month</th>
                                <th>CPC ($)</th>
                                <th>Competition</th>
                                <th>Est. CAC ($)</th>
                                <th>Potential LTV ($)</th>
                                <th>ROI (%)</th>
                                <th>Priority</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
