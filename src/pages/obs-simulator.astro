                            </tr>
                        </tbody>
                    </table>
                </div>

                <!-- Specialties Category -->
                <div class="category-section">
                    <div class="category-header">Specialized Consultations</div>
                    <table class="keyword-table">
                        <thead>
                            <tr>
                                <th>Keyword</th>
                                <th>Volume/month</th>
                                <th>CPC ($)</th>
                                <th>Competition</th>
                                <th>Est. CAC ($)</th>
